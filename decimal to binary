// conversion of decimal number system to binary 
#include<stdio.h>
int main (){
    int n ,rem,bin = 0;
    printf("Enter number ");
    scanf("%d",&n);
    int place = 1;
    int temp = n;      
    while ( n != 0 ){
        rem = n % 2;
        bin = bin + rem * place;
        place = place * 10;
        n = n / 2;  
    }
    printf("the binary of %d is %d",temp,bin);
    return 0;
}
