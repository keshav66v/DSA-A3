//binary to decimal
#include<stdio.h>
#include<math.h>
int main(){
    int n,sum =0,P=0;
    printf("Enter the number ");
    scanf("%d",&n);
    int rem;
    while(n != 0){
        rem = n %10;
        sum += rem * pow(2,P);
        P++;
        n = n/10;
    }
    printf("Decimal number is %d", sum);
    return 0;
}
